<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="450.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="dbsdemo.LoginWindowController">
  <children>
    <TextField fx:id="userNameTextField" layoutX="122.0" layoutY="92.0" prefWidth="200.0" />
    <PasswordField fx:id="passwdTextField" layoutX="123.0" layoutY="163.0" prefWidth="200.0" />
    <ComboBox layoutX="123.0" layoutY="268.0" minWidth="200.0">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ComboBox>
    <Label layoutX="189.0" layoutY="64.0" text="Username">
      <font>
        <Font name="System Bold" size="15.0" fx:id="x1" />
      </font>
    </Label>
    <Label font="$x1" layoutX="189.0" layoutY="136.0" text="Password" />
    <Text layoutX="100.0" layoutY="253.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Or choose an existing guest account:" />
    <Label layoutX="165.0" layoutY="21.0" text="Please log in">
      <font>
        <Font name="System Bold Italic" size="20.0" />
      </font>
    </Label>
    <Button fx:id="loginButton" layoutX="182.0" layoutY="346.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="40.0" prefWidth="81.0" text="Login" />
    <Hyperlink fx:id="goToRegWindowScene" layoutX="139.0" layoutY="189.0" onAction="#goToRegWindowScene" text="Create a new account" textFill="#001899" />
  </children>
</AnchorPane>
